class Solution {
    public int strStr(String haystack, String needle) {
        if(needle.length()>haystack.length()){
            return -1;
        }
        String s="";
         System.out.println("");
        char c=needle.charAt(0);
        for(int i=0;i<haystack;i++){
          char ch=haystack.charAt(0);
          if(ch==c){
              String s=haystack.substring()needle.length(i=1);
          }
        }
    }
    return -1;
}
